<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gmail.appsdmtech.alphadance" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>alphadance</name>
    <description>
       Horários não oficiais das aulas de dança nas unidades da Rede Alpha em: Aracaju, Caminho das Árvores, Costa Azul, Itaigara e Shopping Bella Vista.
    </description>
    <author email="evandrojr@gmail.com" href="http://bit.ly/aphadance">
        Evandro Júnior
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="whatsapp:*" />
    <access launch-external="yes" origin="whatsapp:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="platforms/android/app/src/main/res/mipmap-ldpi/icon.png" />
    <icon density="mdpi" src="platforms/android/app/src/main/res/mipmap-mdpi/icon.png" />
    <icon density="hdpi" src="platforms/android/app/src/main/res/mipmap-hdpi/icon.png" />
    <icon density="xhdpi" src="platforms/android/app/src/main/res/mipmap-xhdpi/icon.png" />
    <icon density="xxhdpi" src="platforms/android/app/src/main/res/mipmap-xxhdpi/icon.png" />
    <icon density="xxxhdpi" src="platforms/android/app/src/main/res/mipmap-xxxhdpi/icon.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
